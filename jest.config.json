{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "collectCoverage": true,
  "bail": true,
  "verbose": true,
  "roots": ["./src"],
  "collectCoverageFrom" : ["/test/*.spec.ts", "!**/node_modules/**", "!**/vendor/**","!./cypress"],
  "setupFilesAfterEnv": ["./setupJest.ts"],
  "transform": {
    ".+\\.(css|scss)$": "jest-css-modules-transform"
  }
}